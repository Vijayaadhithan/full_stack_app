{
  "name": "rest-express",
  "version": "1.0.0",
  "type": "module",
  "license": "MIT",
  "scripts": {
    "dev": "tsx -r dotenv/config server/index.ts",
    "dev:server": "tsx -r dotenv/config server/index.ts",
    "dev:client": "vite",
    "build": "vite build && esbuild server/index.ts --platform=node --packages=external --bundle --format=esm --outdir=dist",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "check": "tsc",
    "db:generate": "drizzle-kit generate",
    "db:migrate": "tsx -r dotenv/config scripts/runMigrations.ts",
    "db:migrate:baseline": "tsx -r dotenv/config scripts/seedMigrationHistory.ts",
    "lint": "eslint \"{client,server,shared}/**/*.{ts,tsx,js}\"",
    "format": "prettier --write \"**/*.{ts,tsx,js,json,md}\"",
    "test": "NODE_ENV=test DATABASE_URL=postgres://localhost/test node --import tsx --test tests/*.test.ts",
    "test:coverage": "NODE_ENV=test DATABASE_URL=postgres://localhost/test c8 --reporter=text --reporter=lcov --check-coverage --lines=30 --functions=30 --branches=30 --statements=30 node --import tsx --test tests/*.test.ts",
    "test:report": "bash scripts/run_tests_with_report.sh",
    "monitor": "node scripts/liveMonitor.js"
  },
